<mapper namespace="com.bitcamp.web.mapperImpl.TxMapperImpl">
	<insert id="insertUserPhone" parameterType="cmd">
 		 INSERT INTO TX VALUES(#{tx.txkey}, #{tx.id},
 		 <choose>
	        <when test="tx.foodSeq == null">
	            ''
	        </when>
	        <otherwise>
	            #{tx.foodSeq}
	        </otherwise>
	    </choose>,
	     <choose>
	        <when test="tx.count == null">
	            '1'
	        </when>
	        <otherwise>
	            #{tx.count}
	        </otherwise>
	    </choose>,
	     <choose>
	        <when test="tx.total == null">
	            ''
	        </when>
	        <otherwise>
	            #{tx.total}
	        </otherwise>
	    </choose>,
	     <choose>
	        <when test="tx.foodSeq == null">
	            ''
	        </when>
	        <otherwise>
	            #{tx.regdate}
	        </otherwise>
	    </choose>
 		  ,#{tx.phoneNum}, #{tx.telecom})
 	</insert>
</mapper>